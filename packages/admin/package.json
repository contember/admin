{
	"name": "@contember/admin",
	"license": "Apache-2.0",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/production/index.js",
	"exports": {
		".": {
			"import": {
				"development": "./dist/development/index.js",
				"production": "./dist/production/index.js"
			}
		},
		"./style.css": "./dist/style.css"
	},
	"files": [
		"dist/"
	],
	"typings": "./dist/admin.d.ts",
	"scripts": {
		"build": "vite build --mode development && vite build --mode production",
		"build:api:dev": "api-extractor run --local && cat src/components/bindingFacade/richText/slate-types.ts | tail -n 7 >> ./dist/admin.d.ts",
		"build:api:prod": "api-extractor run",
		"styles:build": "sass src/index.sass:dist/style.css",
		"test": "jest"
	},
	"dependencies": {
		"@contember/binding": "workspace:*",
		"@contember/client": "workspace:*",
		"@contember/react-client": "workspace:*",
		"@contember/react-multipass-rendering": "workspace:*",
		"@contember/react-utils": "workspace:*",
		"@contember/ui": "workspace:*",
		"@formatjs/icu-messageformat-parser": "2.0.11",
		"@mangoweb/sass-base": "0.0.8",
		"@sindresorhus/slugify": "0.11.0",
		"attr-accept": "2.2.2",
		"blueimp-md5": "2.18.0",
		"buffer": "6.0.3",
		"classnames": "2.3.1",
		"debounce": "1.2.1",
		"fuse.js": "6.4.6",
		"intl-messageformat": "9.9.1",
		"is-hotkey": "0.1.8",
		"leaflet": "1.7.1",
		"path-to-regexp": "6.2.0",
		"react-datepicker": "4.2.1",
		"react-dropzone": "10.2.2",
		"react-error-boundary": "3.1.3",
		"react-leaflet": "2.8.0",
		"react-select": "4.3.1",
		"react-sortable-hoc": "1.11.0",
		"react-window": "1.8.6",
		"slate": "0.73.1",
		"slate-history": "0.66.0",
		"slate-react": "0.74.0",
		"stacktracey": "2.1.7"
	},
	"peerDependencies": {
		"react": "^17",
		"react-dom": "^17"
	},
	"devDependencies": {
		"@types/blueimp-md5": "2.18.0",
		"@types/debounce": "1.2.0",
		"@types/is-hotkey": "0.1.5",
		"@types/leaflet": "1.7.5",
		"@types/react": "17.0.30",
		"@types/react-datepicker": "3.1.8",
		"@types/react-dom": "17.0.9",
		"@types/react-leaflet": "2.8.2",
		"@types/react-select": "4.0.17",
		"@types/react-window": "1.8.5",
		"csstype": "3.0.8",
		"jsdom": "^18.0.0"
	}
}
